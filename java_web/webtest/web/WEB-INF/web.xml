    <servlet>
        <servlet-name>ContextTest</servlet-name>
        <servlet-class>servletContext.ContextTest</servlet-class>
        <!--服务器启动时加载-->
        <load-on-startup>3</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>ContextTest</servlet-name>
        <url-pattern>/ContextTest</url-pattern>
    </servlet-mapping>

    <!--listener监听事件-->
    <listener>
        <listener-class>Listener.MyContextListener</listener-class>
    </listener>
    <listener>
